<script>
// Function to add horizontal lines dynamically between <p> elements
function addHorizontalLines() {
  var paragraphs = document.querySelectorAll('.lowstocklist p');
  paragraphs.forEach(function(para, index) {
    if (index !== paragraphs.length - 1) {
      // Create a <hr> element (horizontal line)
      var hr = document.createElement('hr');
      hr.style.width = '100%'; // Set the width of the <hr> to match the container
      hr.style.margin = '0'; // Remove default margin
      // Insert <hr> before the next <p> element
      para.insertAdjacentElement('afterend', hr);
    }
  });
}

// Call the function initially to add horizontal lines
addHorizontalLines();
</script>

